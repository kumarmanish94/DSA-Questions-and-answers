// code write and see how to visualize in code.
import java.util.*;
public class Arrays{
public static void main(String args[])
{
int[]marks=new int[3];
marks[0]=97;//physics
marks[1]=98;//chemistry
marks[2]=95;//english 

System.out.println(marks[0]);
System.out.println(marks[1]);
System.out.println(marks[2]);
}
}
